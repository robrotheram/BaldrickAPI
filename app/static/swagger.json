    "/bot/": {
      "post": {
        "description": "Get list of something",
        "consumes": [
          "application/json"
        ],
        "produces": [
          "application/json"
        ],
        "schemes": [
          "http"
        ],
        "operationId": "testthing",
        "responses": {
          "200": {
            "$ref": "#/responses/Bot"
          },
          "500": {
            "$ref": "#/responses/errorResponse"
          }
        }
      }
    },
    "/bot/{BotID}": {
      "get": {
        "description": "Get list of something",
        "consumes": [
          "application/json"
        ],
        "produces": [
          "application/json"
        ],
        "schemes": [
          "http"
        ],
        "operationId": "listParams",
        "parameters": [
          {
            "type": "string",
            "name": "BotID",
            "in": "path",
            "required": true
          }
        ],
        "responses": {
          "200": {
            "$ref": "#/responses/Bot"
          },
          "500": {
            "$ref": "#/responses/errorResponse"
          }
        }
      }
    },
    "/bots": {
      "get": {
        "description": "This will show all available pets by default.\nYou can get the pets that are out of stock",
        "produces": [
          "application/json"
        ],
        "schemes": [
          "http"
        ],
        "summary": "Get list of something",
        "operationId": "test",
        "responses": {
          "200": {
            "$ref": "#/responses/Bot"
          },
          "500": {
            "$ref": "#/responses/errorResponse"
          }
        }
      }
    }
  },
  "responses": {
    "Bot": {
      "description": "A ValidationError is an error that is used when the required input fails validation.",
      "headers": {
        "BotName": {
          "type": "string"
        },
        "Type": {
          "type": "string"
        }
      }
    },
    "errorResponse": {
      "description": "A ValidationError is an error that is used when the required input fails validation.",
      "headers": {
        "ErrorCode": {
          "type": "integer",
          "format": "int64"
        },
        "ErrorMessage": {
          "type": "string"
        }
